/* ---------------------------------------------карусель 1----------------------------------------------------------
 */

.radio{
    bottom: 0;
    float: left;
    left: 15px;
    overflow: hidden;
    margin-left: 690px;
    margin-top: 85px;
}

.radio li{
    text-decoration: none;
    display: inline-block;
    font-size: 11px;
    line-height: 14px;
    min-width: 14px;
    background: rgba(0, 0, 0, 0.75);
    color: #3b3942;
    border-radius: 14px;
    padding: 3px;
    text-align: center;
    margin-right: 2px;
    opacity: .75;

    padding-top: 5px 5px 5px 5px;
}

.radio li .active{
	background: #4E443C;
    color: #fff;
    opacity: 1;
    text-shadow: 0 -1px 0 rgba(0, 0, 0, 0.75);

} 

.radio ul{
	    height: 23px;
}


/* ---------------------------------------------------------section-------------------------------------------------------------------------- */

section {
	overflow:hidden;
	position: relative;
	background-image: url(../img/fon_2.png);
	height: 336px;
	width: 100%;
}

/* ---------------------------------------------------sprite---------------------------------------------------------------------------------- */

.sprite{
   	overflow:hidden;
	position: absolute;
}

.sprite li{
	float: left;
	list-style: none;
 	margin-top: 80px;
    margin-right: 300px; 
}

.sprite a{
	height: 28px;
	width: 28px;
	display: block;
	background: url(../img/sprite.png) 0 0 no-repeat;  
}

 .sprite .key{
	background-position: 0px 0;
}

.sprite .spaek{
	background-position: -34.5px 0;
}

.sprite .musik{
	 background-position: -67px 0;
}

