/* <!-- -------------------------------------------------------Menu---------------------------------------------------------- --> */

header{
	background-image: url(../img/header.jpg);
	height: 840px;
	width: 100%;
 	overflow:hidden;
	position: relative; 
 	font-family: 'Myriad Pro', sans-serif; 
 }



 .ping h1{
 	position: absolute; 
 	top: 69px;
    left: 720px; 
    color: #ffffff;
    font-weight: bold 700;
    font-size: 43px;
